{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": { "value": "australiaCentral" },
    "environmentName": { "value": "verity-canberra-env" },
    "apiName": { "value": "verity-canberra-api" },
    "workerName": { "value": "verity-canberra-worker" },
    "acrName": { "value": "veritycanberraacr" },
    "keyVaultName": { "value": "veritycanberrakv" },
    "keyVaultUseRbac": { "value": false },
    "postgresServerName": { "value": "verity-canberra-pg" },
    "postgresDbName": { "value": "verity" },
    "postgresAdminLogin": { "value": "verityadmin" },
    "postgresAdminPassword": { "value": "REPLACE_ME" },
    "postgresPublicNetworkAccess": { "value": "Enabled" },
    "postgresAllowAzureServices": { "value": true },
    "postgresFirewallRules": { "value": [] },
    "redisName": { "value": "verity-canberra-redis" },
    "apiImage": { "value": "veritycanberraacr.azurecr.io/verity-api:latest" },
    "workerImage": { "value": "veritycanberraacr.azurecr.io/verity-api:latest" },
    "apiUrl": { "value": "https://api.yourveritydomain.com" },
    "wsUrl": { "value": "wss://api.yourveritydomain.com" },
    "appUrl": { "value": "https://yourveritydomain.com" },
    "enableFrontDoor": { "value": false },
    "frontDoorProfileName": { "value": "verity-canberra-fd" },
    "frontDoorEndpointName": { "value": "verity-canberra-endpoint" },
    "frontDoorCustomDomain": { "value": "" },
    "enableFrontDoorWaf": { "value": false },
    "frontDoorWafPolicyName": { "value": "verity-canberra-waf" },
    "frontDoorRateLimit": { "value": 200 },
    "frontDoorRateLimitExemptPaths": { "value": ["/webhooks/stripe", "/webhooks/hive"] },
    "jwtSecret": { "value": "REPLACE_ME" },
    "jwtAccessSecret": { "value": "REPLACE_ME" },
    "jwtRefreshSecret": { "value": "REPLACE_ME" },
    "stripeSecretKey": { "value": "REPLACE_ME" },
    "stripeWebhookSecret": { "value": "REPLACE_ME" },
    "stripeSuccessUrl": { "value": "https://yourveritydomain.com/payments/success" },
    "stripeCancelUrl": { "value": "https://yourveritydomain.com/payments/cancel" },
    "stripePriceStarter": { "value": "REPLACE_ME" },
    "stripePricePlus": { "value": "REPLACE_ME" },
    "stripePricePro": { "value": "REPLACE_ME" },
    "agoraAppId": { "value": "REPLACE_ME" },
    "agoraAppCertificate": { "value": "REPLACE_ME" },
    "agoraTokenTtlSeconds": { "value": 45 },
    "hiveApiKey": { "value": "REPLACE_ME" },
    "hiveWebhookSecret": { "value": "REPLACE_ME" },
    "hiveStreamUrl": { "value": "https://your-hive-stream-endpoint" },
    "hiveScreenshotUrl": { "value": "https://your-hive-screenshot-endpoint" }
  }
}
